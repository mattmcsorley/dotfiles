alias secretart='kubectl create secret docker-registry artifactory --docker-server=medcpu-docker.jfrog.io --docker-username=mmcsorley --docker-password=$(cat $HOME/.kube/secret) --docker-email=mmcsorley@pgh.medcpu.com'
alias k="kubectl"
alias kmon="kubectl --namespace=monitoring"
alias kall="kubectl --all-namespaces"
alias klog="kubectl --namespace=logging"
alias kinf="kubectl --namespace=infra"
alias kops="kubectl --namespace=devops"
alias ksys="kubectl --namespace=kube-system"
alias ksno="kubectl --namespace=snomed"
alias koa="kubectl --namespace=oauth2-proxy"
alias kist="kubectl --namespace=istio-system"
alias kes="kubectl --namespace=elasticsearch"
alias kkey="kubectl --namespace=keycloak"
